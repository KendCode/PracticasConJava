package PRACTICA4;
import java.util.Scanner;
public class FibonnaciDeMayorAMenor {

	public static void main(String[] args)
	{
		Scanner leer = new Scanner (System.in);
		int n, a, b, c;
		
		System.out.print("Ingrese el digito: ");
		n = leer.nextInt();
		
		a = 0; b = 1;
		for(int i = 1; i<= n; i++)
		{
			System.out.print(a+", ");
			c = a + b;
			a = b;
			b = c;
		}

	}

}
